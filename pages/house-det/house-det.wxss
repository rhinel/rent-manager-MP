/**house-det.wxss**/

.house-det .weui-form-preview__value {
  text-align: right;
}

.house-det .badge1 {
  padding: 0.05em 0.4em;
  margin-left: 4px;
  vertical-align: top;
  background-color: #576b95;
}

.house-det .weui-media-box__desc {
  -webkit-line-clamp: initial;
}

.house-det .loadmore .weui-loadmore_line {
  margin-top: 1.5em;
  margin-bottom: calc(-0.25em - 10px);
}

.house-det .loadmore {
  overflow: hidden;
}

.house-det .weui-loadmore__tips_in-line {
  background-color: #f8f8f8;
}

.house-det .weui-loadmore_line {
  margin-top: 1.5em;
  margin-bottom: -10px;
}

.house-det .water, .house-det .electric {
  height: 0;
  padding: 0 15px;
  overflow: hidden;
}

.house-det .water-show, .house-det .electric-show {
  height: auto;
  padding: 15px;
}
